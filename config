# Personal account - default configuration
Host github.com
    HostName github.com
    git users
    IdentityFile ~/.ssh/id_rsa_personal

# Work account
Host github.com-work
    HostName github.com
    git users
    IdentityFile ~/.ssh/id_rsa_work

# Personal Config - otheruser 
Host github.com-otheraccount
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_otheraccount

# Personal account - Gitlab host - bonus
Host gitlab.com
    HostName gitlab.com
    git users
    IdentityFile ~/.ssh/id_rsa_gitlab
